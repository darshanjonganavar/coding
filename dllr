#include <stdio.h>
#include <stdlib.h>
struct node {
    int data;
    struct node *prev, *next;
};
struct node *head = NULL;
void insertRear(int x) {
    struct node *new = malloc(sizeof(struct node));
    new->data = x;
    new->next = NULL;
    if (head == NULL) {
        new->prev = NULL;
        head = new;
        return;
    }
    struct node *temp = head;
    while (temp->next != NULL)
        temp = temp->next;
    temp->next = new;
    new->prev = temp;
}
void deleteFront() {
    if (head == NULL) {
        printf("List Empty\n");
        return;
    }
    struct node *temp = head;
    head = head->next;
    if (head != NULL)
        head->prev = NULL;
    free(temp);
}
void display() {
    struct node *temp = head;
    while (temp != NULL) {
        printf("%d ", temp->data);
        temp = temp->next;
    }
    printf("\n");
}
int main() {
    int ch, x;
    while (1) {
        printf("\n1.Insert Rear 2.Delete Front 3.Display 4.Exit\n");
        scanf("%d", &ch);
        switch (ch) {
            case 1: scanf("%d", &x); insertRear(x); break;
            case 2: deleteFront(); break;
            case 3: display(); break;
            case 4: exit(0);
        }
    }
}
