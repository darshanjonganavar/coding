#include <stdio.h>
int adj[10][10], visited[10], n;
void bfs(int s) {
    int q[10], f = 0, r = -1;
    q[++r] = s;
    visited[s] = 1;
    while (f <= r) {
        s = q[f++];
        printf("%d ", s);
        for (int i = 0; i < n; i++) {
            if (adj[s][i] && !visited[i]) {
                visited[i] = 1;
                q[++r] = i;
            }
        }
    }
}
int main() {
    int s;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &adj[i][j]);
    scanf("%d", &s);
    bfs(s);
}
