#include <stdio.h>
#include <stdlib.h>
struct node {
    int data;
    struct node *prev, *next;
};
struct node *head = NULL;
void insertFront(int x) {
    struct node *new = malloc(sizeof(struct node));
    new->data = x;
    new->prev = NULL;
    new->next = head;
    if (head != NULL)
        head->prev = new;
    head = new;
}
void search(int key) {
    struct node *temp = head;
    while (temp != NULL) {
        if (temp->data == key) {
            printf("Element Found\n");
            return;
        }
        temp = temp->next;
    }
    printf("Element Not Found\n");
}
void deleteKey(int key) {
    struct node *temp = head;
    while (temp != NULL && temp->data != key)
        temp = temp->next;
    if (temp == NULL) {
        printf("Element Not Found\n");
        return;
    }
    if (temp->prev != NULL)
        temp->prev->next = temp->next;
    else
        head = temp->next;
    if (temp->next != NULL)
        temp->next->prev = temp->prev;
    free(temp);
}
int main() {
    int n, x, ch;
    printf("Enter number of nodes: ");
    scanf("%d", &n);
    while (n--) {
        scanf("%d", &x);
        insertFront(x);
    }
    while (1) {
        printf("\n1.Search 2.Delete 3.Exit\n");
        scanf("%d", &ch);
        if (ch == 1) { scanf("%d", &x); search(x); }
        else if (ch == 2) { scanf("%d", &x); deleteKey(x); }
        else exit(0);
    }
}
