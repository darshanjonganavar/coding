#include <stdio.h>
#define MAX 5
int q[MAX], front = -1, rear = -1;
void insert() {
    int x;
    if((rear+1)%MAX == front)
        printf("Queue Full\n");
    else {
        printf("Enter element: ");
        scanf("%d", &x);
        if(front == -1) front = 0;
        rear = (rear+1)%MAX;
        q[rear] = x;
    }
}
void delete() {
    if(front == -1)
        printf("Queue Empty\n");
    else {
        printf("Deleted: %d\n", q[front]);
        if(front == rear)
            front = rear = -1;
        else
            front = (front+1)%MAX;
    }
}
void display() {
    int i = front;
    while(i != rear) {
        printf("%d ", q[i]);
        i = (i+1)%MAX;
    }
    printf("%d\n", q[i]);
}
int main() {
    int ch;
    do {
        printf("\n1.Insert\n2.Delete\n3.Display\n4.Exit\n");
        scanf("%d", &ch);
        switch(ch) {
            case 1: insert(); break;
            case 2: delete(); break;
            case 3: display(); break;
        }
    } while(ch != 4);
    return 0;
}
