#include <stdio.h>
#include <string.h>
#define MAX 20
struct Employee 
{
 int key;
 char name[30];
 int occupied;
};
struct Employee HT[MAX];
int hash(int key) 
{
 return key % MAX;
 }
void insert(int key, char name[]) 
{
 int index = hash(key);
 int start = index;
 while (HT[index].occupied == 1) 
 {
 index = (index + 1) % MAX;
 }
 HT[index].key = key;
 strcpy(HT[index].name, name);
 HT[index].occupied = 1;
 printf("Inserted Key %d at index %d\n", key, index);
}
void display() {
 printf("\nHash Table Contents:\n");
 for (int i = 0; i < MAX; i++) {
 if (HT[i].occupied)
 printf("Index %2d → Key: %4d, Name: %s\n", i, HT[i].key, 
HT[i].name);
 else
 printf("Index %2d → Empty\n", i);
 }
}
int main() 
{
 int n, key;
 char name[30];
 for (int i = 0; i < MAX; i++)
 HT[i].occupied = 0;
 printf("Enter number of employee records to insert: ");
 scanf("%d", &n);
 for (int i = 0; i < n; i++)
 {
 printf("\nEnter 4-digit key: ");
 scanf("%d", &key);
 printf("Enter employee name: ");
 scanf("%s", name);
 insert(key, name);
 }
 display();
 return 0;
}

 
