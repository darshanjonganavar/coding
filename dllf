#include <stdio.h>
#include <stdlib.h>
struct node {
    int data;
    struct node *prev, *next;
};
struct node *head = NULL;
void insertFront(int x) {
    struct node *new = malloc(sizeof(struct node));
    new->data = x;
    new->prev = NULL;
    new->next = head;
    if (head != NULL)
        head->prev = new;
    head = new;
}
void deleteRear() {
    if (head == NULL) {
        printf("List Empty\n");
        return;
    }
    struct node *temp = head;
    while (temp->next != NULL)
        temp = temp->next;
    if (temp->prev != NULL)
        temp->prev->next = NULL;
    else
        head = NULL;
    free(temp);
}
void display() {
    struct node *temp = head;
    if (temp == NULL) {
        printf("List Empty\n");
        return;
    }
    while (temp != NULL) {
        printf("%d ", temp->data);
        temp = temp->next;
    }
    printf("\n");
}
int main() {
    int ch, x;
    while (1) {
        printf("\n1.Insert Front 2.Delete Rear 3.Display 4.Exit\n");
        scanf("%d", &ch);
        switch (ch) {
            case 1: printf("Enter value: "); scanf("%d", &x); insertFront(x); break;
            case 2: deleteRear(); break;
            case 3: display(); break;
            case 4: exit(0);
        }
    }
}
