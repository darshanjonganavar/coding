#include <stdio.h>
struct student {
    int roll;
    char name[20];
    int marks;
    char grade;
};
int main() {
    struct student s[20];
    int n, i;
    printf("Enter number of students: ");
    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        printf("Enter roll, name, marks, grade:\n");
        scanf("%d %s %d %c", &s[i].roll, s[i].name, &s[i].marks, &s[i].grade);
    }
    printf("\nStudents scoring >= 75:\n");
    for(i = 0; i < n; i++) {
        if(s[i].marks >= 75)
            printf("%s\n", s[i].name);
    }
    return 0;
}
